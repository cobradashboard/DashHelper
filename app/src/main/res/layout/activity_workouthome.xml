<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".workoutHomepage">

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="412dp"
        android:layout_height="137dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/background4"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="161dp"
            android:layout_height="50dp"
            android:layout_marginTop="90dp"
            android:fontFamily="sans-serif-medium"
            android:text="Workouts"
            android:textAlignment="center"
            android:textColor="#555151"
            android:textSize="25sp"

            android:textStyle="normal|bold" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="384dp"
        android:layout_height="90dp"
        android:layout_margin="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout8"
        app:layout_constraintVertical_bias="0.391">

        <androidx.cardview.widget.CardView
            android:id="@+id/walk"
            android:layout_width="330dp"
            android:layout_height="68dp"
            android:layout_gravity="center"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:clickable="true"
            android:foreground="?android:attr/selectableItemBackground"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="54dp"
                android:layout_height="49dp"
                android:layout_margin="10dp"
                app:srcCompat="@drawable/exercise" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="223dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="90dp"
                android:paddingTop="15dp"
                android:text="Running"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="386dp"
        android:layout_height="99dp"
        android:layout_margin="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout8"
        app:layout_constraintVertical_bias="0.572">

        <androidx.cardview.widget.CardView
            android:id="@+id/run"
            android:layout_width="330dp"
            android:layout_height="68dp"
            android:layout_gravity="center"
            android:layout_marginLeft="28dp"
            android:layout_marginRight="25dp"
            android:clickable="true"
            android:foreground="?android:attr/selectableItemBackground"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="52dp"
                android:layout_height="48dp"
                android:layout_margin="10dp"
                app:srcCompat="@drawable/walk" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="219dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="90dp"
                android:paddingTop="15dp"
                android:text="Walking"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="384dp"
        android:layout_height="88dp"
        android:layout_margin="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout8"
        app:layout_constraintVertical_bias="0.751">

        <androidx.cardview.widget.CardView
            android:id="@+id/cycle"
            android:layout_width="330dp"
            android:layout_height="68dp"
            android:layout_gravity="center"
            android:layout_marginLeft="28dp"
            android:layout_marginRight="25dp"
            android:clickable="true"
            android:foreground="?android:attr/selectableItemBackground"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:layout_margin="10dp"
                app:srcCompat="@drawable/bike" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="215dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="90dp"
                android:paddingTop="15dp"
                android:text="Cycling"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="384dp"
        android:layout_height="85dp"
        android:layout_margin="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout8"
        app:layout_constraintVertical_bias="0.914">

        <androidx.cardview.widget.CardView
            android:id="@+id/swim"
            android:layout_width="334dp"
            android:layout_height="69dp"
            android:layout_gravity="center"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:clickable="true"
            android:foreground="?android:attr/selectableItemBackground"
            app:cardCornerRadius="10dp">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="56dp"
                android:layout_height="48dp"
                android:layout_margin="10dp"
                app:srcCompat="@drawable/swimmer" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="200dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="90dp"
                android:paddingTop="25dp"
                android:text="Swimming"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="325dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="Necessary Workouts"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout8"
        app:layout_constraintVertical_bias="0.697" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="363dp"
        android:layout_height="53dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="-Exercising cause the body to make chemicals that can help a person to feel good-"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.156"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout8"
        app:layout_constraintVertical_bias="0.277" />


</androidx.constraintlayout.widget.ConstraintLayout>