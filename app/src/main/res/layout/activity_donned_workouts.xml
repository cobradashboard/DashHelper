<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".donnedWorkout">

    <LinearLayout
        android:id="@+id/linearLayout8"
        android:layout_width="412dp"
        android:layout_height="132dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/board1"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="271dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="90dp"
            android:fontFamily="sans-serif-medium"
            android:text="@string/donnedworkoutplan"
            android:textAlignment="center"
            android:textColor="@color/lightgray"
            android:textSize="25sp"
            android:textStyle="normal|bold"
            tools:text="@string/donnedworkoutplan" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="369dp"
        android:layout_height="92dp"
        android:layout_marginTop="228dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.617"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout8"
        app:layout_constraintVertical_bias="0.666">

        <androidx.cardview.widget.CardView
            android:id="@+id/VP1"
            android:layout_width="match_parent"
            android:layout_height="77dp"
            android:background="#4F5058">

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="75dp"
                android:layout_height="68dp"
                android:layout_marginRight="250dp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="225dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="10dp"
                android:paddingTop="10dp"
                android:text="@string/plan1"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textStyle="bold"
                tools:text="@string/plan1" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="369dp"
        android:layout_height="90dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.764"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout8"
        app:layout_constraintVertical_bias="0.651">

        <androidx.cardview.widget.CardView
            android:id="@+id/VP2"
            android:layout_width="match_parent"
            android:layout_height="77dp">

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="75dp"
                android:layout_height="68dp"
                android:layout_marginRight="250dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="225dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="10dp"
                android:paddingTop="10dp"
                android:text="@string/plan2"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textStyle="bold"
                tools:text="@string/plan2" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="369dp"
        android:layout_height="90dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout9"
        app:layout_constraintVertical_bias="0.251">

        <androidx.cardview.widget.CardView
            android:id="@+id/VP3"
            android:layout_width="match_parent"
            android:layout_height="77dp">

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="75dp"
                android:layout_height="68dp"
                android:layout_marginRight="250dp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="225dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="10dp"
                android:paddingTop="20dp"
                android:text="@string/plan3"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textStyle="bold"
                tools:text="@string/plan3" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="346dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="#00442E2E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.212" />



</androidx.constraintlayout.widget.ConstraintLayout>