    package="com.example.dashhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".showCourseDetails"></activity>
        <activity android:name=".newaddexam_view" />
        <activity android:name=".view_result" />
        <activity android:name=".addexam_page" />
        <activity android:name=".addexam_homepage" />
        <activity android:name=".newaddcourseview" />
        <activity android:name=".addcoursepage" />
        <activity android:name=".addcourse_homepage" />
        <activity android:name=".timetable" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>